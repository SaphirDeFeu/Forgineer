{
  "schemaVersion": 1,
  "id": "forgineer",
  "version": "${version}",
  "name": "Forgineer",
  "description": "We yearn for the mines.",
  "authors": [ "SaphirDeFeu" ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/forgineer/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "io.github.saphirdefeu.forgineer.client.ForgineerDataGenerator"
    ],
    "client": [
      "io.github.saphirdefeu.forgineer.client.ForgineerClient"
    ],
    "main": [
      "io.github.saphirdefeu.forgineer.Forgineer"
    ]
  },
  "mixins": [
    "forgineer.mixins.json",
    {
      "config": "forgineer.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  },
  "accessWidener" : "forgineer.accesswidener"
}
